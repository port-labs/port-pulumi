{
    "compilerOptions": {
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "files": [
        "action.ts",
        "actionPermissions.ts",
        "aggregationProperties.ts",
        "blueprint.ts",
        "config/index.ts",
        "config/vars.ts",
        "entity.ts",
        "index.ts",
        "provider.ts",
        "scorecard.ts",
        "team.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts",
        "webhook.ts"
    ]
}
